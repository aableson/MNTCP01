<?xml version="1.0" encoding="UTF-8"?><questestinterop><objectbank ident="QLIB_50893" xmlns:d2l_2p0="http://desire2learn.com/xsd/d2lcp_v2p0"><item d2l_2p0:id="1" ident="OBJ_115806" label="QUES_47774_56306" d2l_2p0:page="1" title="Testing Latex"><itemmetadata><qtimetadata><qti_metadatafield><fieldlabel>qmd_computerscored</fieldlabel><fieldentry>yes</fieldentry></qti_metadatafield><qti_metadatafield><fieldlabel>qmd_questiontype</fieldlabel><fieldentry>Multiple Choice</fieldentry></qti_metadatafield><qti_metadatafield><fieldlabel>qmd_weighting</fieldlabel><fieldentry>1.000000000</fieldentry></qti_metadatafield><qti_metadatafield><fieldlabel>qmd_globalid</fieldlabel><fieldentry>1ad5a6e5-e006-47b4-9f05-462c926fb17f</fieldentry></qti_metadatafield><qti_metadatafield><fieldlabel>qmd_displayid</fieldlabel><fieldentry>MNTC_P04 2015/16 W-397</fieldentry></qti_metadatafield></qtimetadata></itemmetadata><itemproc_extension><d2l_2p0:difficulty>1</d2l_2p0:difficulty><d2l_2p0:isbonus>no</d2l_2p0:isbonus><d2l_2p0:ismandatory>no</d2l_2p0:ismandatory></itemproc_extension><presentation><flow><material><mattext texttype="text/html">&lt;p&gt;Testing the latex inline with &lt;math title=""  xmlns="http://www.w3.org/1998/Math/MathML" display="block"&gt;&lt;semantics&gt;&lt;mstyle&gt;
  &lt;mrow class="MJX-TeXAtom-ORD"&gt;
    &lt;mo&gt;$&lt;/mo&gt;
  &lt;/mrow&gt;
  &lt;mi&gt;y&lt;/mi&gt;
  &lt;mo&gt;=&lt;/mo&gt;
  &lt;msup&gt;
    &lt;mi&gt;x&lt;/mi&gt;
    &lt;mn&gt;2&lt;/mn&gt;
  &lt;/msup&gt;
&lt;/mstyle&gt;&lt;annotation encoding="latex"&gt;{"version":"1.1","math":"$y = x^2$"}&lt;/annotation&gt;&lt;/semantics&gt;&lt;/math&gt;and then moving on.&lt;/p&gt;
&lt;p&gt;&lt;/p&gt;</mattext></material><response_extension><d2l_2p0:display_style>2</d2l_2p0:display_style><d2l_2p0:enumeration>6</d2l_2p0:enumeration><d2l_2p0:grading_type>0</d2l_2p0:grading_type></response_extension><response_lid ident="QUES_47774_56306_LID" rcardinality="Single"><render_choice shuffle="no"><flow_label class="Block"><response_label ident="QUES_47774_56306_A177000"><flow_mat><material><mattext texttype="text/html">&lt;p&gt;And testing again...&lt;math title="" xmlns="http://www.w3.org/1998/Math/MathML" display="inline"&gt;&lt;semantics&gt;&lt;mstyle&gt;
  &lt;mn&gt;14&lt;/mn&gt;
  &lt;msup&gt;
    &lt;mi&gt;x&lt;/mi&gt;
    &lt;mn&gt;3&lt;/mn&gt;
  &lt;/msup&gt;
&lt;/mstyle&gt;&lt;annotation encoding="latex"&gt;{&amp;quot;version&amp;quot;:&amp;quot;1.1&amp;quot;,&amp;quot;math&amp;quot;:&amp;quot;\(14x^3\)&amp;quot;}&lt;/annotation&gt;&lt;/semantics&gt;&lt;/math&gt;afterwards&lt;/p&gt;</mattext></material></flow_mat></response_label></flow_label><flow_label class="Block"><response_label ident="QUES_47774_56306_A177001"><flow_mat><material><mattext texttype="text/html">&lt;p&gt;and another &lt;math title="" xmlns="http://www.w3.org/1998/Math/MathML" display="inline"&gt;&lt;semantics&gt;&lt;mstyle&gt;
  &lt;mi&gt;y&lt;/mi&gt;
  &lt;mo&gt;=&lt;/mo&gt;
  &lt;msup&gt;
    &lt;mi&gt;e&lt;/mi&gt;
    &lt;mi&gt;x&lt;/mi&gt;
  &lt;/msup&gt;
&lt;/mstyle&gt;&lt;annotation encoding="latex"&gt;{&amp;quot;version&amp;quot;:&amp;quot;1.1&amp;quot;,&amp;quot;math&amp;quot;:&amp;quot;\(y = e^x\)&amp;quot;}&lt;/annotation&gt;&lt;/semantics&gt;&lt;/math&gt;test with brackets&lt;/p&gt;</mattext></material></flow_mat></response_label></flow_label><flow_label class="Block"><response_label ident="QUES_47774_56306_A177002"><flow_mat><material><mattext texttype="text/html">&lt;p&gt;with a dollar &lt;math title="" xmlns="http://www.w3.org/1998/Math/MathML" display="block"&gt;&lt;semantics&gt;&lt;mstyle&gt;
  &lt;mrow class="MJX-TeXAtom-ORD"&gt;
    &lt;mo&gt;$&lt;/mo&gt;
  &lt;/mrow&gt;
  &lt;mi&gt;y&lt;/mi&gt;
  &lt;mo&gt;=&lt;/mo&gt;
  &lt;msup&gt;
    &lt;mi&gt;x&lt;/mi&gt;
    &lt;mn&gt;2&lt;/mn&gt;
  &lt;/msup&gt;
&lt;/mstyle&gt;&lt;annotation encoding="latex"&gt;{&amp;quot;version&amp;quot;:&amp;quot;1.1&amp;quot;,&amp;quot;math&amp;quot;:&amp;quot;$y = x^2$&amp;quot;}&lt;/annotation&gt;&lt;/semantics&gt;&lt;/math&gt;sign&lt;/p&gt;</mattext></material></flow_mat></response_label></flow_label></render_choice></response_lid></flow></presentation><resprocessing><respcondition title="Response Condition 1"><conditionvar><varequal respident="QUES_47774_56306_LID">QUES_47774_56306_A177000</varequal></conditionvar><setvar action="Set">0.000000000</setvar><displayfeedback feedbacktype="Response" linkrefid="QUES_47774_56306_IF177000" /></respcondition><respcondition title="Response Condition 2"><conditionvar><varequal respident="QUES_47774_56306_LID">QUES_47774_56306_A177001</varequal></conditionvar><setvar action="Set">100.000000000</setvar><displayfeedback feedbacktype="Response" linkrefid="QUES_47774_56306_IF177001" /></respcondition><respcondition title="Response Condition 3"><conditionvar><varequal respident="QUES_47774_56306_LID">QUES_47774_56306_A177002</varequal></conditionvar><setvar action="Set">0.000000000</setvar><displayfeedback feedbacktype="Response" linkrefid="QUES_47774_56306_IF177002" /></respcondition></resprocessing><itemfeedback ident="QUES_47774_56306_IF177000"><material><mattext texttype="text/html" /></material></itemfeedback><itemfeedback ident="QUES_47774_56306_IF177001"><material><mattext texttype="text/html" /></material></itemfeedback><itemfeedback ident="QUES_47774_56306_IF177002"><material><mattext texttype="text/html" /></material></itemfeedback></item><section d2l_2p0:id="2" ident="SECT_119460" title="Section01"><sectionproc_extension><d2l_2p0:display_section_name>no</d2l_2p0:display_section_name><d2l_2p0:display_section_line>no</d2l_2p0:display_section_line><d2l_2p0:type_display_section>0</d2l_2p0:type_display_section></sectionproc_extension><item d2l_2p0:id="3" ident="OBJ_115255" label="QUES_47571_56076" d2l_2p0:page="1" title="Question 1"><itemmetadata><qtimetadata><qti_metadatafield><fieldlabel>qmd_computerscored</fieldlabel><fieldentry>yes</fieldentry></qti_metadatafield><qti_metadatafield><fieldlabel>qmd_questiontype</fieldlabel><fieldentry>Multiple Choice</fieldentry></qti_metadatafield><qti_metadatafield><fieldlabel>qmd_weighting</fieldlabel><fieldentry>1.000000000</fieldentry></qti_metadatafield><qti_metadatafield><fieldlabel>qmd_globalid</fieldlabel><fieldentry>8c61469b-13e8-4b17-825d-7a96d1cf3bdd</fieldentry></qti_metadatafield><qti_metadatafield><fieldlabel>qmd_displayid</fieldlabel><fieldentry>MNTC_P04 2015/16 W-396</fieldentry></qti_metadatafield></qtimetadata></itemmetadata><itemproc_extension><d2l_2p0:difficulty>1</d2l_2p0:difficulty><d2l_2p0:isbonus>no</d2l_2p0:isbonus><d2l_2p0:ismandatory>no</d2l_2p0:ismandatory></itemproc_extension><presentation><flow><material><mattext texttype="text/html">&lt;p&gt;The graph of &lt;math xmlns="http://www.w3.org/1998/Math/MathML"&gt;&lt;mrow&gt;&lt;mi&gt;f&lt;/mi&gt;&lt;mrow&gt;&lt;mo form="prefix"&gt;(&lt;/mo&gt;&lt;mi&gt;x&lt;/mi&gt;&lt;mo form="postfix"&gt;)&lt;/mo&gt;&lt;/mrow&gt;&lt;/mrow&gt;&lt;/math&gt; is shown below. &lt;br /&gt; &lt;img src="http://www.mast.queensu.ca/~ableson/MNTCP04/CheckIns/Week09/AntiderivativeGraph_Original.png" alt="Graphs" width="300px" /&gt;&lt;/p&gt;
&lt;p&gt;Which of the following graphs (a)-(d) could represent an &lt;b&gt;anti&lt;/b&gt;derivative of &lt;math xmlns="http://www.w3.org/1998/Math/MathML"&gt;&lt;mrow&gt;&lt;mi&gt;f&lt;/mi&gt;&lt;mrow&gt;&lt;mo form="prefix"&gt;(&lt;/mo&gt;&lt;mi&gt;x&lt;/mi&gt;&lt;mo form="postfix"&gt;)&lt;/mo&gt;&lt;/mrow&gt;&lt;/mrow&gt;&lt;/math&gt;? &lt;br /&gt; &lt;img src="http://www.mast.queensu.ca/~ableson/MNTCP04/CheckIns/Week09/Question1_AllGraphs.png" alt="Graphs" width="500px" /&gt;&lt;/p&gt;</mattext></material><response_extension><d2l_2p0:display_style>2</d2l_2p0:display_style><d2l_2p0:enumeration>6</d2l_2p0:enumeration><d2l_2p0:grading_type>0</d2l_2p0:grading_type></response_extension><response_lid ident="QUES_47571_56076_LID" rcardinality="Single"><render_choice shuffle="no"><flow_label class="Block"><response_label ident="QUES_47571_56076_A176085"><flow_mat><material><mattext texttype="text/html">&lt;p&gt;(a)&lt;/p&gt;</mattext></material></flow_mat></response_label></flow_label><flow_label class="Block"><response_label ident="QUES_47571_56076_A176086"><flow_mat><material><mattext texttype="text/html">&lt;p&gt;(b)&lt;/p&gt;</mattext></material></flow_mat></response_label></flow_label><flow_label class="Block"><response_label ident="QUES_47571_56076_A176087"><flow_mat><material><mattext texttype="text/html">&lt;p&gt;(c)&lt;/p&gt;</mattext></material></flow_mat></response_label></flow_label><flow_label class="Block"><response_label ident="QUES_47571_56076_A176088"><flow_mat><material><mattext texttype="text/html">&lt;p&gt;(d)&lt;/p&gt;</mattext></material></flow_mat></response_label></flow_label></render_choice></response_lid></flow></presentation><resprocessing><respcondition title="Response Condition 1"><conditionvar><varequal respident="QUES_47571_56076_LID">QUES_47571_56076_A176085</varequal></conditionvar><setvar action="Set">0.000000000</setvar><displayfeedback feedbacktype="Response" linkrefid="QUES_47571_56076_IF176085" /></respcondition><respcondition title="Response Condition 2"><conditionvar><varequal respident="QUES_47571_56076_LID">QUES_47571_56076_A176086</varequal></conditionvar><setvar action="Set">0.000000000</setvar><displayfeedback feedbacktype="Response" linkrefid="QUES_47571_56076_IF176086" /></respcondition><respcondition title="Response Condition 3"><conditionvar><varequal respident="QUES_47571_56076_LID">QUES_47571_56076_A176087</varequal></conditionvar><setvar action="Set">0.000000000</setvar><displayfeedback feedbacktype="Response" linkrefid="QUES_47571_56076_IF176087" /></respcondition><respcondition title="Response Condition 4"><conditionvar><varequal respident="QUES_47571_56076_LID">QUES_47571_56076_A176088</varequal></conditionvar><setvar action="Set">100.000000000</setvar><displayfeedback feedbacktype="Response" linkrefid="QUES_47571_56076_IF176088" /></respcondition></resprocessing><itemfeedback ident="QUES_47571_56076"><material><mattext texttype="text/html">&lt;p&gt;The values of &lt;math xmlns="http://www.w3.org/1998/Math/MathML"&gt;&lt;mrow&gt;&lt;mi&gt;f&lt;/mi&gt;&lt;mrow&gt;&lt;mo form="prefix"&gt;(&lt;/mo&gt;&lt;mi&gt;x&lt;/mi&gt;&lt;mo form="postfix"&gt;)&lt;/mo&gt;&lt;/mrow&gt;&lt;/mrow&gt;&lt;/math&gt; are positive, then negative, so the &lt;b&gt;slopes&lt;/b&gt; of the anti-derivative are first positive (increasing function) then negative (decreasing function). &lt;br /&gt; When the value of the function is zero (at &lt;math xmlns="http://www.w3.org/1998/Math/MathML"&gt;&lt;mrow&gt;&lt;mi&gt;x&lt;/mi&gt;&lt;mo&gt;=&lt;/mo&gt;&lt;mn&gt;3&lt;/mn&gt;&lt;/mrow&gt;&lt;/math&gt;), the anti-derivative isn't changing (zero slope).&lt;/p&gt;</mattext></material></itemfeedback><itemfeedback ident="QUES_47571_56076_IF176085"><material><mattext texttype="text/html" /></material></itemfeedback><itemfeedback ident="QUES_47571_56076_IF176086"><material><mattext texttype="text/html" /></material></itemfeedback><itemfeedback ident="QUES_47571_56076_IF176087"><material><mattext texttype="text/html" /></material></itemfeedback><itemfeedback ident="QUES_47571_56076_IF176088"><material><mattext texttype="text/html" /></material></itemfeedback></item></section></objectbank></questestinterop>